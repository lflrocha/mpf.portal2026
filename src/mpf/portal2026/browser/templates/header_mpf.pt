            <svg
              viewBox="0 0 573 186"
              class="h-auto w-10 sm:w-12 lg:w-[64px]"
              xmlns="http://www.w3.org/2000/svg"
            >
              <path d="M235.73 185.29H185.16L174 105.22C173.22 99.57 172.5 93.29 171.86 86.37C171.21 79.46 169.8 71.91 169.28 63.72C168.07 71.57 165.69 81.35 162.14 93.07C161.36 95.52 160.8 97.33 160.45 98.51L134.62 185.28H100.92L75.09 98.51C74.74 97.33 74.22 95.52 73.53 93.07C69.9 81.35 67.52 71.61 66.39 63.84C65.87 70.84 64.74 77.78 64.05 84.65C63.36 91.53 62.53 98.37 61.58 105.21L50.42 185.28H0L33.69 0.0800018L83.87 0L111.61 95.59C111.78 96.26 112.13 97.36 112.65 98.88C115.33 107.4 117.02 114.69 117.71 120.76C118.05 117.47 118.71 113.83 119.66 109.82C120.61 105.81 121.91 100.99 123.55 95.33L151.57 0.0699921H202.22L235.72 185.27L235.73 185.29Z" fill="var(--brand)"/>
              <path d="M319.99 185.29H269.35V0.0898438H325.98C348.79 0.0898438 364.8 1.09985 374.02 3.12985C383.23 5.14985 391.05 8.61984 397.47 13.5098C405.01 19.1698 410.76 26.2298 414.72 34.7098C418.68 43.1898 420.65 52.6199 420.65 62.9899C420.65 74.0399 418.7 83.7699 414.79 92.1599C410.88 100.55 405.11 107.45 397.47 112.85C391.69 116.9 384.9 119.81 377.08 121.58C369.26 123.35 356.74 124.23 339.51 124.23H319.98V185.29H319.99ZM319.99 83.5398H330.6C343.98 83.5398 353.33 81.8698 358.64 78.5298C363.94 75.1998 366.59 69.5098 366.59 61.4798C366.59 53.4498 363.98 48.1799 358.78 44.8799C353.57 41.5899 344.36 39.9398 331.16 39.9398H320V83.5298L319.99 83.5398Z" fill="var(--brand)"/>
              <path d="M454.63 185.29V0.0898438H572.28V40.6299H505.1V72.8499H567.3V112.54H505.1V185.29H454.63Z" fill="var(--brand)"/>
            </svg>
          </span>

          <span class="min-w-0">
            <span class="block font-semibold leading-tight text-[color:var(--brand)] text-base sm:text-lg lg:text-xl truncate mpf-brand__name">
              Ministério Público Federal
            </span>

            <span class="hidden sm:block text-gray-700 leading-tight truncate text-xs sm:text-sm mpf-brand__tagline">
              Portal Institucional
            </span>
          </span>
        </a>

        <!-- Mobile: botão menu -->
        <button
          type="button"
          data-menu-button="true"
          class="lg:hidden relative z-[70] inline-flex items-center justify-center rounded-md border border-gray-200 bg-white px-3 py-2 text-sm font-medium text-gray-900 hover:bg-gray-50
            focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30"
          aria-controls="mobile-menu"
          aria-expanded="false"
          aria-label="Abrir menu"
        >
          <span class="sr-only">Abrir menu</span>
          <span class="mpf-burger" aria-hidden="true">
            <span></span><span></span><span></span>
          </span>
        </button>
      </div>

      <!-- Desktop: links institucionais -->
      <nav aria-label="Links institucionais" class="hidden lg:flex justify-center">
        <ul class="flex items-center text-sm text-gray-700 divide-x divide-gray-300/70">
          <li class="pr-3">
            <a href="./paginas-internas/interna.html" class="hover:underline focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30 rounded"
              tal:attributes="href string:${context/portal_url}/servicos/acesso-a-informacao">
              Acesso à Informação
            </a>
          </li>
          <li class="px-3">
            <a href="https://transparencia.mpf.mp.br" target="_blank"  class="hover:underline focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30 rounded">
              Transparência
            </a>
          </li>
          <li class="pl-3">
            <a href="./paginas-internas/interna.html" class="hover:underline focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30 rounded"
              tal:attributes="href string:${context/portal_url}/contact-info">
              Contatos
            </a>
          </li>
        </ul>
      </nav>

      <!-- Acessibilidade: desktop grande -->
      <div class="a11y-desktop items-center justify-end gap-2 text-sm">
        <a href="./paginas-internas/interna.html" class="text-gray-700 hover:underline focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30 rounded"
          tal:attributes="href string:${context/portal_url}/acessibilidade">
          Acessibilidade
        </a>

        <span aria-hidden="true" class="mx-1 h-4 w-px bg-gray-300/70"></span>

        <button
          type="button"
          data-contrast-toggle
          role="switch"
          aria-checked="false"
          aria-label="Ativar alto contraste"
          title="Alto contraste"
          class="inline-flex h-9 w-9 items-center justify-center rounded-md border border-gray-200 bg-white text-gray-800 hover:bg-gray-50
            focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30"
        >
          <svg aria-hidden="true" viewBox="0 0 24 24" class="h-5 w-5" fill="none" stroke="currentColor" stroke-width="2">
            <path d="M21 12.8A8.5 8.5 0 1 1 11.2 3a6.5 6.5 0 1 0 9.8 9.8Z" />
          </svg>
        </button>

        <button
          type="button"
          data-font="decrease"
          aria-label="Diminuir tamanho do texto"
          title="Diminuir tamanho do texto"
          class="inline-flex h-9 w-9 items-center justify-center rounded-md border border-gray-200 bg-white text-gray-800 hover:bg-gray-50
            focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30"
        >
          <span aria-hidden="true" class="font-semibold">A−</span>
        </button>

        <button
          type="button"
          data-font="reset"
          aria-label="Restaurar tamanho do texto"
          title="Restaurar tamanho do texto"
          class="inline-flex h-9 w-9 items-center justify-center rounded-md border border-gray-200 bg-white text-gray-800 hover:bg-gray-50
            focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30"
        >
          <span aria-hidden="true" class="font-semibold">A</span>
        </button>

        <button
          type="button"
          data-font="increase"
          aria-label="Aumentar tamanho do texto"
          title="Aumentar tamanho do texto"
          class="inline-flex h-9 w-9 items-center justify-center rounded-md border border-gray-200 bg-white text-gray-800 hover:bg-gray-50
            focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30"
        >
          <span aria-hidden="true" class="font-semibold">A+</span>
        </button>



        <a
          href="./paginas-internas/interna.html"
          aria-label="Mapa do site"
          title="Mapa do site"
          class="inline-flex h-9 w-9 items-center justify-center rounded-md border border-gray-200 bg-white text-gray-800 hover:bg-gray-50
            focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30"
          tal:attributes="href string:${context/portal_url}/sitemap"
        >
          <svg aria-hidden="true" viewBox="0 0 24 24" class="h-5 w-5" fill="none" stroke="currentColor" stroke-width="2">
            <path d="M9 18l-6 3V6l6-3 6 3 6-3v15l-6 3-6-3Z" />
            <path d="M9 3v15" />
            <path d="M15 6v15" />
          </svg>
        </a>
      </div>

      <!-- Acessibilidade: dropdown abaixo de lg -->
      <div class="a11y-mobile relative flex justify-end" data-dd-root="true">
        <button
          type="button"
          class="inline-flex items-center gap-2 rounded-md border border-gray-200 bg-white px-3 py-2 text-sm text-gray-900 hover:bg-gray-50
            focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30"
          data-dd-button="dd-a11y"
          aria-haspopup="menu"
          aria-expanded="false"
          aria-controls="dd-a11y"
        >
          Acessibilidade
          <svg aria-hidden="true" class="ml-1 inline-block h-3.5 w-3.5 opacity-70" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.75" stroke-linecap="round" stroke-linejoin="round">
            <path d="M6 9l6 6 6-6" />
          </svg>
        </button>

        <div id="dd-a11y" data-dd-menu="true" role="menu" hidden="hidden" class="absolute right-0 top-full mt-2 w-72 rounded-md border border-gray-200 bg-white shadow-lg p-2 z-[80]">
          <button role="menuitem" type="button" data-contrast-toggle aria-label="Ativar alto contraste" class="w-full text-left rounded-md px-3 py-2 text-sm text-gray-900 hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30">
            Alto contraste
          </button>

          <div class="grid grid-cols-3 gap-2 px-1 py-2">
            <button type="button" data-font="decrease" class="rounded-md border border-gray-200 px-3 py-2 text-sm hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30" aria-label="Diminuir texto">
              A−
            </button>
            <button type="button" data-font="reset" class="rounded-md border border-gray-200 px-3 py-2 text-sm hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30" aria-label="Restaurar texto">
              A
            </button>
            <button type="button" data-font="increase" class="rounded-md border border-gray-200 px-3 py-2 text-sm hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30" aria-label="Aumentar texto">
              A+
            </button>
          </div>



          <a role="menuitem" href="./paginas-internas/interna.html" class="block rounded-md px-3 py-2 text-sm text-gray-900 hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30"
            tal:attributes="href string:${context/portal_url}/sitemap">
            Mapa do site
          </a>
        </div>
      </div>
    </div>
  </div>
</div>

<!-- BARRA INFERIOR (menu principal + busca) -->
<div class="mpf-bottombar bg-[color:var(--mpf-blue)]/[0.07] border-t border-[color:var(--mpf-blue)]/20">
  <div class="mx-auto max-w-[1440px] px-4">
    <div class="flex items-center justify-between gap-4">

      <!-- Menu principal (desktop) -->
      <nav aria-label="Navegação principal" class="hidden lg:block">
        <ul class="flex items-stretch flex-nowrap"
            tal:define="items view/menu_tree">

          <li class="relative" data-dd-root="true"
              tal:repeat="item items">
              <tal:block tal:define="
                ddid string:dd-${repeat/item/index};
                accid string:${ddid}-acc
              ">

            <button type="button"
                    class="mpf-nav-item gap-2"
                    tal:attributes="
                      data-dd-button ddid;
                      aria-controls ddid
                    "
                    aria-haspopup="menu"
                    aria-expanded="false">
              <span tal:content="item/title">Item</span>
              <svg aria-hidden="true" class="ml-1 inline-block h-3.5 w-3.5 opacity-70"
                  xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none"
                  stroke="currentColor" stroke-width="1.75" stroke-linecap="round" stroke-linejoin="round">
                <path d="M6 9l6 6 6-6" />
              </svg>
            </button>

            <!-- Dropdown nível 1 -->
            <div tal:attributes="id ddid"
                data-dd-menu="true"
                role="menu"
                hidden="hidden"
                class="absolute left-0 top-full mt-1 w-80 rounded-md border border-gray-300 bg-white shadow-lg p-2"
                tal:condition="item/children">

              <tal:n2 repeat="child item/children">

                <!-- Nível 2 simples -->
                <tal:cond tal:condition="not: child/children">
                  <a role="menuitem"
                    class="block rounded-md px-3 py-2 text-sm text-gray-900 hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-[color:var(--brand)]/30"
                    tal:attributes="href child/url"
                    tal:content="child/title">
                    Página
                  </a>
                </tal:cond>

                <!-- Nível 2 com filhos (vira accordion dentro do dropdown) -->
                <tal:cond tal:condition="child/children">
                  <div class="mt-1 rounded-md"
                      tal:define="acc2id string:${ddid}-child-${repeat/child/index}">

                    <button type="button"
                            class="w-full text-left rounded-md px-3 py-2 text-sm text-gray-900 hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-[color:var(--brand)]/30 flex items-center justify-between"
                            tal:attributes="
                              data-acc-button acc2id;
                              aria-controls acc2id
                            "
                            aria-expanded="false">
                      <span tal:content="child/title">Seção</span>
                      <svg aria-hidden="true" class="ml-1 inline-block h-3.5 w-3.5 opacity-70"
                          xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none"
                          stroke="currentColor" stroke-width="1.75" stroke-linecap="round" stroke-linejoin="round">
                        <path d="M6 9l6 6 6-6" />
                      </svg>
                    </button>

                    <div tal:attributes="id acc2id"
                        data-acc-panel="true"
                        hidden="hidden"
                        class="mt-1 ml-3 border-l border-gray-200 pl-3 grid gap-1">

                      <tal:n3 repeat="gchild child/children">
                        <a role="menuitem"
                          class="block rounded-md px-3 py-2 text-sm text-gray-900 hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-[color:var(--brand)]/30"
                          tal:attributes="href gchild/url"
                          tal:content="gchild/title">
                          Subpágina
                        </a>
                      </tal:n3>

                    </div>
                  </div>
                </tal:cond>

              </tal:n2>
            </div>
          </tal:block>
          </li>
        </ul>
      </nav>

      <!-- Busca (desktop) -->
      <form class="hidden md:flex items-center gap-2 ml-auto" role="search" aria-label="Buscar no site"
        tal:attributes="action string:${context/portal_url}/@@search">
        <label class="sr-only" for="site-search">Buscar no Portal do MPF</label>
        <input
          id="site-search"
          name="SearchableText"
          type="search"
          placeholder="Buscar…"
          class="w-72 rounded-md border border-gray-200 bg-white px-3 py-2 text-sm outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30"
        />

        <button type="submit" class="rounded-md bg-[color:var(--mpf-blue)] px-4 py-2 text-sm font-medium text-white hover:opacity-95 focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30">
          Buscar
        </button>
      </form>

      <!-- Mobile hint -->
      <div class="md:hidden text-sm text-gray-600 py-3">
        Use o menu para acessar páginas e busca.
      </div>
    </div>
  </div>
</div>

<!-- MOBILE OVERLAY -->
<div data-menu-overlay="true" class="fixed inset-0 z-50 bg-black/40" hidden="hidden"></div>

<!-- MOBILE PANEL -->
<nav
  id="mobile-menu"
  data-menu-panel="true"
  class="fixed right-0 top-0 z-[60] h-full w-[88%] max-w-sm bg-white shadow-xl border-l border-gray-200 overflow-y-auto overscroll-contain"
  hidden="hidden"
  tabindex="-1"
  aria-label="Menu mobile"
>
  <div class="p-4 border-b border-gray-200 flex items-center justify-between">
    <p class="font-semibold text-gray-900">Menu</p>

    <button
      type="button"
      data-menu-close="true"
      class="inline-flex items-center justify-center rounded-md border border-gray-200 bg-white px-3 py-2 text-sm font-semibold text-gray-900 hover:bg-gray-50
        focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30"
      aria-label="Fechar menu"
    >
      Fechar
    </button>
  </div>

  <div class="p-4 grid gap-6 pb-10">
    <!-- Busca mobile -->
    <form role="search" aria-label="Buscar no site (mobile)"
      tal:attributes="action string:${context/portal_url}/@@search">
      <label class="sr-only" for="site-search-mobile">Buscar</label>
      <input
        id="site-search-mobile"
        name="SearchableText"
        type="search"
        placeholder="Buscar…"
        aria-label="Buscar no site (mobile)"
        class="w-full rounded-md border border-gray-200 bg-white px-3 py-2 text-sm outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30"
      />
      <button
        type="submit"
        class="mt-2 w-full rounded-md bg-[color:var(--mpf-blue)] px-4 py-2 text-sm font-medium text-white hover:opacity-95
          focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30"
      >
        Buscar
      </button>
    </form>

    <!-- Links institucionais (mobile) -->
    <section aria-label="Links institucionais" class="grid gap-2 text-sm">
      <p class="font-semibold text-gray-900">Links institucionais</p>
      <a class="rounded-md px-3 py-2 hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30" href="./paginas-internas/interna.html"
        tal:attributes="href string:${context/portal_url}/servicos/acesso-a-informacao">Acesso à Informação</a>
      <a class="rounded-md px-3 py-2 hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30" href="https://www.transparencia.mpf.mp.br"
        target="_blank">Transparência</a>
      <a class="rounded-md px-3 py-2 hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30" href="./paginas-internas/interna.html"
        tal:attributes="href string:${context/portal_url}/contact-info">Contatos</a>
    </section>

    <!-- Ferramentas -->
    <section aria-label="Ferramentas de acessibilidade" class="grid gap-2 text-sm">
      <a href="./paginas-internas/interna.html" class="font-semibold text-gray-900"
        tal:attributes="href string:${context/portal_url}/accessibility-info"
        >Acessibilidade</a>

      <button
        type="button"
        data-contrast-toggle
        role="switch"
        aria-checked="false"
        class="w-full rounded-md border border-gray-200 px-4 py-3 text-left hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30"
      >
        Alto contraste
      </button>

      <div class="grid grid-cols-3 gap-2">
        <button type="button" data-font="decrease" class="rounded-md border border-gray-200 px-3 py-3 hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30" aria-label="Diminuir tamanho do texto">A-</button>
        <button type="button" data-font="reset" class="rounded-md border border-gray-200 px-3 py-3 hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30" aria-label="Restaurar tamanho do texto">A</button>
        <button type="button" data-font="increase" class="rounded-md border border-gray-200 px-3 py-3 hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30" aria-label="Aumentar tamanho do texto">A+</button>
      </div>

      <a href="./paginas-internas/interna.html" class="w-full rounded-md border border-gray-200 px-4 py-3 hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30"
        tal:attributes="href string:${context/portal_url}/sitemap"
        >Mapa do site</a>
    </section>

    <!-- Navegação principal (mobile) com 2 níveis -->
    <section aria-label="Navegação principal" class="grid gap-2 text-sm">
      <p class="font-semibold text-gray-900">Páginas</p>

      <tal:def tal:define="items view/menu_tree">

        <tal:rep1 repeat="item items">
          <tal:block tal:define="mid string:mobile-m-${repeat/item/index}">

            <div class="rounded-md border border-gray-200 overflow-hidden">
              <button type="button"
                      class="w-full px-3 py-3 flex items-center justify-between hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30"
                      tal:attributes="
                        data-acc-button mid;
                        aria-controls mid
                      "
                      aria-expanded="false">
                <span class="font-semibold text-gray-900" tal:content="item/title">Item</span>
                <svg aria-hidden="true" class="ml-1 inline-block h-3.5 w-3.5 opacity-70"
                    xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none"
                    stroke="currentColor" stroke-width="1.75" stroke-linecap="round" stroke-linejoin="round">
                  <path d="M6 9l6 6 6-6" />
                </svg>
              </button>

              <div tal:attributes="id mid"
                  data-acc-panel="true"
                  hidden="hidden"
                  class="p-2 grid gap-1">

                <a class="rounded-md px-3 py-2 hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30"
                  tal:condition="item/url"
                  tal:attributes="href item/url">
                  <span tal:content="item/title">Ver página</span>
                </a>

                <tal:n2 repeat="child item/children">

                  <tal:cond tal:condition="not: child/children">
                    <a class="rounded-md px-3 py-2 hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30"
                      tal:attributes="href child/url"
                      tal:content="child/title">
                      Subpágina
                    </a>
                  </tal:cond>

                  <tal:cond tal:condition="child/children">
                    <tal:block tal:define="mid2 string:${mid}-${repeat/child/index}">
                      <div class="rounded-md border border-gray-200 overflow-hidden mt-1">

                        <button type="button"
                                class="w-full px-3 py-3 flex items-center justify-between hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30"
                                tal:attributes="
                                  data-acc-button mid2;
                                  aria-controls mid2
                                "
                                aria-expanded="false">
                          <span class="text-gray-900" tal:content="child/title">Seção</span>
                          <svg aria-hidden="true" class="ml-1 inline-block h-3.5 w-3.5 opacity-70"
                              xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none"
                              stroke="currentColor" stroke-width="1.75" stroke-linecap="round" stroke-linejoin="round">
                            <path d="M6 9l6 6 6-6" />
                          </svg>
                        </button>

                        <div tal:attributes="id mid2"
                            data-acc-panel="true"
                            hidden="hidden"
                            class="p-2 grid gap-1">

                          <tal:n3 repeat="gchild child/children">
                            <a class="rounded-md px-3 py-2 hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-[color:var(--mpf-blue)]/30"
                              tal:attributes="href gchild/url"
                              tal:content="gchild/title">
                              Neto
                            </a>
                          </tal:n3>

                        </div>
                      </div>
                    </tal:block>
                  </tal:cond>

                </tal:n2>

              </div>
            </div>

          </tal:block>
        </tal:rep1>


      </tal:def>
    </section>

  </div>
